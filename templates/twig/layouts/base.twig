		<title>{{ page.title }}</title>

		{% block css %}
			<link rel="stylesheet" type="text/css" href="{{ config.urls.templates }}styles/tailwind.css"/>
		{% endblock %}

		{% block js %}
			<script src="{{ config.urls.templates }}scripts/main.js"></script>
